# GNBSim iptables rules

*filter
:FORWARD ACCEPT [0:0]
-A FORWARD -i {{ gnbsim.router.data_iface }} -o gnbaccess -j ACCEPT
-A FORWARD -i gnbaccess -o {{ gnbsim.router.data_iface }} -j ACCEPT
{% if inventory_hostname in groups['master_nodes'] %}
-A FORWARD -i gnbaccess -o access -j ACCEPT
-A FORWARD -i access -o gnbaccess -j ACCEPT
{% endif %}
COMMIT
